COMMAND
</property>
<property type="boolean" id="enabled" mode="w" >
True
</property>
<property type="string" id="component" mode="w" >
${here/id}
</property>
<property type="string" id="eventClass" mode="w" >
/Status
</property>
<property type="int" id="severity" mode="w" >
3
</property>
